{
  "private": true,
  "name": "electron-app",
  "version": "0.0.0",
  "main": "lib/backend/electron-main.js",
  "dependencies": {
    "@theia/core": "1.67.0",
    "@theia/editor": "1.67.0",
    "@theia/electron": "1.67.0",
    "@theia/filesystem": "1.67.0",
    "@theia/markers": "1.67.0",
    "@theia/messages": "1.67.0",
    "@theia/monaco": "1.67.0",
    "@theia/navigator": "1.67.0",
    "@theia/plugin-ext": "1.67.0",
    "@theia/plugin-ext-vscode": "1.67.0",
    "@theia/preferences": "1.67.0",
    "@theia/process": "1.67.0",
    "@theia/terminal": "1.67.0",
    "@theia/workspace": "1.67.0",
    "hello-world": "0.0.0"
  },
  "devDependencies": {
    "@theia/cli": "1.67.0",
    "electron": "^38.4.0"
  },
  "scripts": {
    "bundle": "npm run rebuild && theia build --mode development",
    "rebuild": "theia rebuild:electron --cacheRoot ..",
    "start": "theia start",
    "watch": "npm run rebuild && theia build --watch --mode development"
  },
  "theia": {
    "target": "electron",
    "generator": {
      "config": {
        "preloadPlugins": [
          "../plugins/calm-theme",
          "../plugins/material-icon-theme/extension/extension",
          "../plugins/material-theme/extension/extension"
        ]
      }
    }
  }
}