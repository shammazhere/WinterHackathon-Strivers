{
    "name": "Calm Dark",
    "type": "dark",
    "colors": {
        "activityBar.background": "#090B10",
        "activityBar.foreground": "#d7dae0",
        "activityBarBadge.background": "#A3E635",
        "activityBarBadge.foreground": "#000000",
        "editor.background": "#0F111A",
        "editor.foreground": "#d7dae0",
        "editor.lineHighlightBackground": "#171b26",
        "editor.selectionBackground": "#404859",
        "editorCursor.foreground": "#A3E635",
        "editorGroupHeader.tabsBackground": "#090B10",
        "editorIndentGuide.background": "#2c323c",
        "editorLineNumber.foreground": "#495162",
        "list.activeSelectionBackground": "#1F232F",
        "list.activeSelectionForeground": "#ffffff",
        "list.hoverBackground": "#171b26",
        "list.inactiveSelectionBackground": "#171b26",
        "panel.background": "#090B10",
        "panel.border": "#000000",
        "sideBar.background": "#090B10",
        "sideBar.foreground": "#8F93A2",
        "sideBarSectionHeader.background": "#090B10",
        "sideBarSectionHeader.foreground": "#d7dae0",
        "statusBar.background": "#090B10",
        "statusBar.foreground": "#676E95",
        "statusBar.noFolderBackground": "#090B10",
        "tab.activeBackground": "#0F111A",
        "tab.activeForeground": "#ffffff",
        "tab.border": "#090B10",
        "tab.inactiveBackground": "#090B10",
        "tab.inactiveForeground": "#676E95",
        "titleBar.activeBackground": "#090B10",
        "titleBar.activeForeground": "#d7dae0",
        "titleBar.inactiveBackground": "#090B10",
        "titleBar.inactiveForeground": "#676E95"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#546E7A",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#82AAFF"
            }
        },
        {
            "scope": [
                "variable",
                "variable.other",
                "variable.language"
            ],
            "settings": {
                "foreground": "#d7dae0"
            }
        },
        {
            "scope": [
                "variable.other.php"
            ],
            "settings": {
                "foreground": "#82AAFF"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted.single",
                "string.quoted.double"
            ],
            "settings": {
                "foreground": "#C3E88D"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#FFCB6B"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.class"
            ],
            "settings": {
                "foreground": "#A3E635"
            }
        },
        {
            "scope": [
                "punctuation",
                "meta.brace",
                "meta.delimiter"
            ],
            "settings": {
                "foreground": "#89DDFF"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#F78C6C"
            }
        }
    ]
}